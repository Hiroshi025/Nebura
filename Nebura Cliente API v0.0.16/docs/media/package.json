{
  "name": "nebura-assistent",
  "version": "0.0.19",
  "main": "src/main.js",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev:debug": "ts-node-dev --respawn --inspect=0.0.0.0:9229 --project tsconfig.json --require tsconfig-paths/register src/main.ts",
    "docs": "typedoc",
    "prepare": "husky",
    "release": "standard-version",
    "build": "npm run clean && npm run compile && npm run copy-assets",
    "build:check": "tsc && tsc-alias -p tsconfig.json && npm run sentry:sourcemaps",
    "build:prod": "NODE_ENV=production npm run build",
    "build:dev": "NODE_ENV=development npm run build",
    "build:staging": "NODE_ENV=staging npm run build",
    "compile": "tsc -b tsconfig.build.json",
    "compile:watch": "tsc -b tsconfig.build.json --watch",
    "clean": "rimraf build",
    "clean:full": "npm run clean && rimraf node_modules/.cache",
    "copy-assets": "copyfiles -u 1 \"src/**/*.{graphql,json,png,jpg,svg,html,css,env}\" build",
    "copy-env": "copyfiles -u 1 \"config/**/*.env\" build/config",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --inspect --require tsconfig-paths/register src/main.ts",
    "start:debug": "NODE_OPTIONS='--inspect' npm run start",
    "prebuild": "npm run type-check",
    "postbuild": "npm run copy-env",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org nebura --project nebura-client ./build && sentry-cli sourcemaps upload --org nebura --project nebura-client ./build"
  },
  "keywords": [
    "api",
    "modular",
    "discord",
    "whatsapp",
    "github",
    "google-ai",
    "typescript",
    "nodejs",
    "express",
    "socket.io",
    "swagger"
  ],
  "author": "Hiroshi025",
  "license": "ISC",
