{
  "name": "nebura-works",
  "version": "0.0.12",
  "main": "index.js",
  "scripts": {
    "test": "jest && npm run docs",
    "dev:debug": "ts-node-dev --respawn --inspect=0.0.0.0:9229 --project tsconfig.json --require tsconfig-paths/register src/main.ts",
    "docs": "typedoc",
    "lint": "eslint --ext .ts .",
    "prepare": "husky",
    "release": "standard-version",
    "build": "npm run clean && npm run compile && npm run copy-assets",
    "build:check": "tsc && tsc-alias -p tsconfig.json",
    "build:prod": "NODE_ENV=production npm run build",
    "build:dev": "NODE_ENV=development npm run build",
    "build:staging": "NODE_ENV=staging npm run build",
    "compile": "tsc -b tsconfig.build.json",
    "compile:watch": "tsc -b tsconfig.build.json --watch",
    "clean": "rimraf dist",
    "clean:full": "npm run clean && rimraf node_modules/.cache",
    "copy-assets": "copyfiles -u 1 \"src/**/*.{graphql,json,png,jpg,svg,html,css,env}\" dist",
    "copy-env": "copyfiles -u 1 \"config/**/*.env\" dist/config",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --inspect --require tsconfig-paths/register src/main.ts",
    "start:debug": "NODE_OPTIONS='--inspect' npm run start",
    "prebuild": "npm run type-check",
    "postbuild": "npm run copy-env"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "schema": "src/structure/database/prisma/schema.prisma"
  },
  "imports": {},
  "description": "Este proyecto es una API modular que integra múltiples servicios como Discord, WhatsApp, GitHub, Google AI, y más. A continuación, se detallan las funcionalidades, rutas, comandos y eventos disponibles en el proyecto.",
  "engines": {
    "node": ">=20.18.0",
    "vscode": "^1.22.0"
